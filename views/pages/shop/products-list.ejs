<!DOCTYPE html>
<html>
<head>
  <% include ../../includes/header.ejs %>
</head>

<body>
    <% include ../../includes/nav.ejs %>
  <div class="jumbotron text-center">
    <div class="container">
      <h1><%= title %></h1>
    </div>
  </div>
    <div>
        <h1>Products</h1>
        <% if (products.length > 0) { %>
            <ul>
                <% products.forEach((products) => { %>
                    <h3> Item: <%= products.title %> </h3>
                <% }) %>
            </ul>
        <% } else { %>
            <h1>No products found!</h1>
        <% } %>
    </div>

</body>
</html>
